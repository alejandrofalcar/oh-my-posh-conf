{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "upgrade": {
    "source": "cdn",
    "interval": "168h",
    "auto": false,
    "notice": false
  },
  "blocks": [
    {
      "type": "prompt",
      "alignment": "right",
      "overflow": "hide",
      "segments": [
        {
          "properties": {
            "style": "unique"
          },
          "leading_diamond": "\ue0b6",
          "trailing_diamond": "\ue0b4",
          "template": "\ue5ff {{ .Path }}",
          "foreground": "#000000",
          "background": "#ffc515",
          "type": "path",
          "style": "diamond"
        },
        {
          "properties": {
            "branch_template": "{{ trunc 25 .Branch }}",
            "fetch_stash_count": true,
            "fetch_status": true,
            "fetch_upstream_icon": true
          },
          "leading_diamond": " \ue0b6",
          "trailing_diamond": "\ue0b4",
          "template": " {{ .UpstreamIcon }} {{ .HEAD }}{{if .BranchStatus }} {{ .BranchStatus }}{{ end }}{{ if .Working.Changed }} \uf044 {{ .Working.String }}{{ end }}{{ if and (.Working.Changed) (.Staging.Changed) }} |{{ end }}{{ if .Staging.Changed }} \uf046 {{ .Staging.String }}{{ end }}{{ if gt .StashCount 0 }} \ueb4b {{ .StashCount }}{{ end }} ",
          "foreground": "#FFFFFF",
          "background": "#170782",
          "type": "git",
          "style": "diamond"
        },
        {
          "properties": {
            "fetch_version": false
          },
          "leading_diamond": " \ue0b6",
          "trailing_diamond": "\ue0b4",
          "template": "\ue626{{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }}",
          "foreground": "#7FD5EA",
          "background": "#29315A",
          "type": "go",
          "style": "diamond"
        },
        {
          "properties": {
            "fetch_version": true
          },
          "leading_diamond": " \ue0b6",
          "trailing_diamond": "\ue0b4",
          "template": "\ue718 {{ if .PackageManagerIcon }}{{ .PackageManagerIcon }} {{ end }}{{ .Full }}",
          "foreground": "#ffffff",
          "background": "#078e15",
          "type": "node",
          "style": "diamond"
        }
      ]
    },
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "leading_diamond": "\ue0b6",
          "trailing_diamond": "\ue0b4",
          "template": "{{ .UserName }} ‚ùØ",
          "foreground": "#2e2390",
          "background": "#ffffff",
          "type": "text",
          "style": "diamond"
          
        }
      ],
      "newline": true
    }
  ],
  "version": 3,
  "final_space": true
}
